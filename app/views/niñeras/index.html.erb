<p id="notice"><%= notice %></p>

<h1>Niñeras</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Edad</th>
      <th>Genero</th>
      <th>Fecha</th>
      <th>De</th>
      <th>A</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @niñeras.each do |niñera| %>
      <tr>
        <td><%= niñera.nombre %></td>
        <td><%= niñera.edad %></td>
        <td><%= niñera.genero %></td>
        <td><%= niñera.fecha %></td>
        <td><%= niñera.de %></td>
        <td><%= niñera.a %></td>
        <td><%= link_to 'Show', niñera %></td>
        <td><%= link_to 'Edit', edit_niñera_path(niñera) %></td>
        <td><%= link_to 'Destroy', niñera, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Niñera', new_niñera_path %>
